package immutability;

public class ThreadSafeImmutability {
	
}

class ImmutableInt {
	private int value;
	
	public void ImmutableInt (int value) {
		this.value = value;	
	}
	
	public int getValue () {
		return this.value;
	}
	
	public ImmutableInt add (int newValue) {
		return new ImmutableInt (value + newValue);
	}
}


