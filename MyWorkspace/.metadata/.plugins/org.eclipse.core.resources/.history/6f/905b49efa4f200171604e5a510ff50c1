package inheritance;

class Foo {
	
	int i =10;
	
	public void method1(){
		System.out.println("FOO.Method1");
	}
	
	public void method2(){
		System.out.println("FOO.Method2");
	}

}

 class Boo extends Foo {
	int i = 20;
	
	public void method1(){
		System.out.println("Boo.Method1");
	}
	
	public void method3(){
		System.out.println("BOO.Method3");
	}

}
 
 public class Test {
		public static void main(String a[]) {
			
			//Boo b = new Foo();
			//Foo f = new Boo();
	        //f.method1();
			//f.method2();
			//f.method3();
		System.out.println(f.i);
			
		}
 }
