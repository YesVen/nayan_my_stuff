package nestedClass.localClass;

public class TestClass {

	public void doSomething (String abc) {
		String test="test";
		class LocalClass{
			public void print(String xyz) {
				System.out.println(test+xyz+abc);
			}
		}
		
		LocalClass c = new LocalClass();
		c.print("test");
	}
	
	public static void main(a[]) {
		
	}
	
}
