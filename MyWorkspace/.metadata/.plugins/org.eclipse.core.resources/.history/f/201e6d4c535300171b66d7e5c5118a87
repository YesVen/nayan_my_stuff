package cloning;

public class TestShallowCloning {

}

class Person {
	String name;
	int age;
	Address address;
	Education education;
	
}

class Address {
	String city;
	String street;
}

class Education {
	String school;
	String college;
	
	Education (String school, String college) {
		this.school = school;
		this.college = college;
	}
	
	public String toString () {
		return " Studied in: " + this.school + " and " + this.college;
	}
}
